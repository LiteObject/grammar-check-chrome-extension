{
    "manifest_version": 2,
    "name": "Grammar Check Extension",
    "version": "1.0",
    "description": "A simple grammar check tool",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "browser_action": {
      "default_title": "Read text",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      },
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]    
  }